{
    "name": "davidecesarano/embryo-framework",
    "type": "project",
    "description": "Embryo is a simple PHP framework for building web applications.",
    "homepage": "https://github.com/davidecesarano/embryo-framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Cesarano",
            "email": "davide.cesarano@unipegaso.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "davidecesarano/embryo-http": "dev-master",
        "davidecesarano/embryo-middleware": "dev-master",
        "davidecesarano/embryo-container": "dev-master",
        "davidecesarano/embryo-emitter": "dev-master",
        "davidecesarano/embryo-routing": "dev-master",
        "davidecesarano/embryo-pdo": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Embryo\\": "Embryo/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpstan/phpstan": "^0.12.42"
    }
}
